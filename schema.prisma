generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  id        String   @id @default(uuid())
  email     String   @unique
  username  String
  files     String[] @default([])
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
